{
    "version": "0.2.0",
    "configurations": [
    {
        "type": "node",
        "name": "vscode-jest-tests.v2",
        "request": "launch",
        "program": "${workspaceFolder}/node_modules/.bin/jest",
        "args": [
            "--runInBand",
            "--watchAll=false",
            "--testNamePattern",
            "${jest.testNamePattern}",
            "--runTestsByPath",
            "${jest.testFile}"
        ],
        "cwd": "${workspaceFolder}",
        "console": "integratedTerminal",
        "internalConsoleOptions": "neverOpen",
        "disableOptimisticBPs": true,
        "windows": {
            "program": "${workspaceFolder}/node_modules/jest/bin/jest"
        }
    },
    
        
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Program",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}\\555Movie\\src\\store\\useWeekly.ts",
            "outFiles": [
                "${workspaceFolder}/**/*.js"
            ]
        },
        {
            "type": "node",
            "name": "JavaScript Debug Terminal",
            "request": "launch",
            "cwd": "${workspaceFolder}"
        }
    ]
}